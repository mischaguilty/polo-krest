{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bastinald/laravel-livewire-ui": "^2.4.16",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.4",
        "guzzlehttp/guzzle": "^7.3.0",
        "laravel-notification-channels/telegram": "^0.5.1",
        "laravel/framework": "^8.51.0",
        "laravel/tinker": "^2.6.1",
        "lorisleiva/laravel-actions": "^2.1.5",
        "propaganistas/laravel-phone": "^4.3.0",
        "spatie/laravel-translatable": "^5.0.1"
    },
    "require-dev": {
        "facade/ignition": "^2.11.2",
        "fakerphp/faker": "^v1.15.0",
        "laravel/sail": "^v1.15.0",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.5.0",
        "phpunit/phpunit": "^9.5.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
